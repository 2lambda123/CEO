include ../common.mk

nwsrc = filename.nw

all: lib tex

lib: $(nwsrc) $(header) $(obj)
	$(AR) rcs $(libsrc) $(obj)

python: $(nwsrc)
	$(TANGLE) -Rceo.pxd $(nwsrc) >> $(CEOPYPATH)/ceo.pxd
	$(TANGLE) -Rceo.pyx $(nwsrc) >> $(CEOPYPATH)/ceo.pyx

tex: $(texsrc)

clean:
	rm -f $(obj) $(header) *.*~

cleanbins:
	rm -f bins/*

